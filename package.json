    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.5",
    "prettier": "3.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^8.1.6",
